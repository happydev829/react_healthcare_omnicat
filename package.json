{
  "name": "omnicat",
  "version": "0.0.0",
  "description": "At Home Ai Guided Biometrics Optimization",
  "homepage": "https://omnicat.netlify.com",
  "repository": "https://github.com/tidelake/omnicat",
  "keywords": [
    "healthcare",
    "blockchain",
    "ai"
  ],
  "authors": [
    "Jesse Crockett <crockett.jesse@gmail.com>",
    "Hari Muralidharan <help@hari.com.au>"
  ],
  "license": "MIT",
  "main": "src/index.html",
  "scripts": {
    "start": "parcel -p 3000 watch src/index.html",
    "build": "parcel build src/index.html",
    "lint": "eslint src",
    "built": "http-server -s -g -p 3000 dist",
    "hint": "hint http://localhost:3000",
    "test": "echo 'not working yet' # npm run built && npm-run-all -r -p http-server hint"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.9.0",
    "sass": "^1.18.0",
    "store": "^2.0.12",
    "truffle-contract": "^4.0.11",
    "web3": "^1.0.0-beta2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "animista": "^0.0.1",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-babel": "^8.0.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^1.3.1",
    "openzeppelin-solidity": "^2.2.0",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "tar": "^4.4.8"
  },
  "optionalDependencies": {
    "acorn": "^6.1.1",
    "ajv": "^5.5.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "prettier",
    "jsx-a11y",
    "react-hooks"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "eslintIgnore": [
    "test/*",
    "migrations/*",
    "node_modules/*"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "prettier": {
    "no-semi": true,
    "tab-width": 2,
    "use-tabs": false,
    "arrow-parens": "avoid",
    "jsx-bracket-same-line": "false",
    "bracket-spacing": true,
    "trailing-commas": "es5",
    "single-quote": true,
    "jsx-single-quote": true
  },
  "rules": {
    "prettier/prettier": 0,
    "react-hooks/rules-of-hooks": 1,
    "prefer-const": 1,
    "no-return-assign": 1,
    "no-console": 0,
    "no-alert": 0,
    "semi": 0,
    "comma-dangle": 1,
    "no-unused-vars": 1,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/accessible-emoji": 0
  }
}
